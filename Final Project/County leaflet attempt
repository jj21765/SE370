library(sf)

county_shape <- st_read("cb_2018_us_county_500k.shp")
county_shape <- county_shape %>%
  mutate("fips" = COUNTYFP) %>%
  mutate(fips = trimws(as.character(fips)))%>%
  arrange(fips)

